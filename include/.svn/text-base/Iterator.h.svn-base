#ifndef _ITERATOR_H_
#define _ITERATOR_H_

#include "Core.h"
	

namespace I
{
	template <class T> class Iterator 
	{
	public:
		virtual T& Current() const = 0;
		virtual void Next() = 0;
		virtual bool Valid() const = 0;
		T& operator *() { return Current(); }
		void operator ++() { Next(); }
	};
};
#endif