#ifndef _HASHABLE_H_
#define _HASHABLE_H_

#include "EqualityComparable.h"

namespace I 
{
	template <typename T> class Hashable : public EqualityComparable<T>
	{
	public: 
		virtual unsigned int GetHashCode() = 0;
	};
	
	template <typename T> class HashGenerator
	{
	public:
		virtual unsigned int GetHashCode(const T& o) = 0;
	};
};

#endif