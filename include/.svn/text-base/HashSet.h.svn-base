#ifndef _HASHSET_H_
#define _HASHSET_H_

#include "Foundation/include/HashMap.h"
#include "Foundation/include/MapSet.h"
#include <cstdarg>

namespace I
{
	template <typename T, template<typename> class C = EqualityComparator> class HashSet : public MapSet<HashMap, T, C>
	{
	public:
		static HashSet<T> FromValues(const T& first, ...) {
			va_list ap;
			HashSet<T> r;
			va_start(ap, first);
			for (const T& i = first; &i != NULL; va_arg(ap, const T&))
				r.Add(i);
			return r;
		};
	};
};

#endif